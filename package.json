    "mac",
    "linux",
    "windows"
  ],
  "authors": [
    "Eugene Ware <eugene@noblesamurai.com>",
    "contributors"
  ],
  "contributors": [
    "Jannis R <mail@jannisr.de>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/eugeneware/ffmpeg-static/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "any-shell-escape": "^0.1.1",
    "tape": "^4.11.0"
  }
}
