{
  "name": "ffmpeg-static",
  "version": "3.0.0",
  "description": "ffmpeg static binaries for Mac OSX and Linux and Windows",
  "main": "index.js",
  "files": [
    "index.js",
    "example.js",
    "bin"
  ],
  "scripts": {
    "build": "./build/index.sh",
